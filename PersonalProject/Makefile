.PHONY: clean All

All:
	@echo "----------Building project:[ Banking_Record_System - Debug ]----------"
	@cd "Banking_Record_System" && "$(MAKE)" -f  "Banking_Record_System.mk"
clean:
	@echo "----------Cleaning project:[ Banking_Record_System - Debug ]----------"
	@cd "Banking_Record_System" && "$(MAKE)" -f  "Banking_Record_System.mk" clean
